{
  "env": {
    "node": true,
    "es2022": true,
    "jest": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-undef": "error",
    "no-prototype-builtins": "off"
  },
  "overrides": [
    {
      "files": ["__tests__/**/*.js"],
      "env": {
        "jest": true
      }
    }
  ]
}
